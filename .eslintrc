{
    "extends": "airbnb-base",
    "plugins": [
        "react"
    ],
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "globals": {
        "document": false,
        "navigator": false,
        "window": false
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-uses-vars": 2,
        "no-var": 0,
        "class-methods-use-this": 0,
        "object-shorthand": 0,
        "max-len": 0,
        "func-names": 0,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "prefer-template": 0,
        "prefer-arrow-callback": 0,
        "no-param-reassign": 0,
        "no-unused-vars": [
            2,
            {
                "args": "none"
            }
        ],
        "comma-dangle": 0,
        "no-console": 0,
        "quote-props": [
            2,
            "consistent"
        ],
        "react/jsx-uses-react": 2,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "function-paren-newline": 0
    }
}